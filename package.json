{
  "name": "next-with-split",
  "version": "0.0.0-development",
  "description": "This is a plugin for split testing (AB testing) in Next.js.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aiji42/next-with-split.git"
  },
  "keywords": [
    "next.js",
    "next",
    "react",
    "plugins",
    "vercel",
    "ab-test",
    "split-test",
    "ab test",
    "split test"
  ],
  "author": "aiji42 <aiji42@gmail.com> (https://twitter.com/aiji42_dev)",
  "bugs": {
    "url": "https://github.com/aiji42/next-with-split/issues"
  },
  "homepage": "https://github.com/aiji42/next-with-split#readme",
  "scripts": {
    "lint": "eslint ./src/**/*.ts ./src/*.ts",
    "format": "prettier './src/**/*.ts' './src/*.ts' --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc && ts-add-module-exports",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "nookies": "^2.5.2"
  },
  "peerDependencies": {
    "next": ">=10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "next": "^11.0.1",
    "prettier": "^2.3.2",
    "semantic-release": "^18.0.0",
    "semantic-release-cli": "^5.4.3",
    "ts-add-module-exports": "^1.0.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
